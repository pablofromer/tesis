/// Students and Professors///

/// Rules ///
s(?Y) :- t(?X, ?Y).
t(?X, ?Z):- p(?X).

/// Queries ///
Q_0(?A) :- t(?A, ?B), s(?B).
Q_1(?A) :- t(?A, ?B).
Q_2(?A) :- p(?A).
Q_3(?A) :- s(?A).
Q_4(?A,?B) :- t(?A, ?B), s(?A), p(?B).
Q_5(?B) :- t(?A, ?B), t(?B, ?C), p(?A), p(?C).
Q_C(?A) :- t(?A, 'c').

?- Q_0(?A).
?- Q_1(?A).
?- Q_2(?A).
?- Q_3(?A).
?- Q_4(?A,?B).
?- Q_5(?B).
?- Q_C(?A).

